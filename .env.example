# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Gemini AI Configuration (for property notes analysis)
VITE_GEMINI_API_KEY=your_gemini_api_key_here

# Development Settings
NODE_ENV=development

# Feature Flags (safe defaults)
# Enable the new secondary market (holds/reservations) flow
VITE_FLAG_SECONDARY_MARKET=false
# Show trading UI (buttons/sections); actual flow still controlled by SECONDARY_MARKET flag
VITE_FLAG_TRADING_UI=true
# Enforce 14-day trial gating for dashboard/marketplace access
VITE_FLAG_TRIAL_ENFORCEMENT=false
# Enable in-app notifications improvements (bell + alerts list)
VITE_FLAG_NOTIFICATIONS=true
# Enable AnalyticsService (user_events + last_activity tracking)
VITE_FLAG_ANALYTICS=false
